@page "/locationdetail/{Id}"

<h1>Location Detail - @Location.PhysicalAddress</h1> 

<div class="row">
    <div class="col-md-12">
        <div class="form-group row">
            <label class="col-md-4">Id</label>
            <div class="col-md-8">
                <label class="form-control">@Location.Id</label>
            </div>
        </div>
        <div class="form-group row">
            <label class="col-md-4">Physical Address</label>
            <div class="col-md-8">
                <label class="form-control">@Location.PhysicalAddress</label>
            </div>
        </div>
        <div class="form-group row">
            <label class="col-md-4">ZipCode</label>
            <div class="col-md-8">
                <label class="form-control">@Location.ZipCode</label>
            </div>
        </div>
        <div class="form-group row">
            <label class="col-md-4">City</label>
            <div class="col-md-8">
                <label class="form-control">@Location.City</label>
            </div>
        </div>
        <div class="form-group row">
            <label class="col-md-4">State</label>
            <div class="col-md-8">
                <label class="form-control">@Location.State</label>
            </div>
        </div>
        <div class="form-group row">
            <label class="col-md-4">Country</label>
            <div class="col-md-8">
                <label class="form-control">@Location.Country</label>
            </div>
        </div>
    </div>
</div>